$SETGLOBAL GP_YLABEL "Degree in C"
$SETGLOBAL GP_XRANGE '1750:2000'
$SETGLOBAL GP_YRANGE '-1.0:1.5'

$libinclude gnupltxy OUTPUT YEAR T2M
$offtext

