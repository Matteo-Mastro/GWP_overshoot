********** ACC2 version 4.2: ACC2_outputs_future.inc *******************************************************************
********** File outputs to display results (future mode) ===============================================================

FILE OUT_FUTURE_A /out_future_a.dat/;
OUT_FUTURE_A.AP=0;
OUT_FUTURE_A.PW=1000;
PUT OUT_FUTURE_A;
LOOP(T, PUT T.TL:5,
CON.L(T,'CO2'):8:3,
CON.L(T,'CH4'):9:3,
CON.L(T,'N2O'):8:3,
CON.L(T,'rOH'):8:3,
CON.L(T,'CF4'):8:3,
CON.L(T,'C2F6'):8:3,
CON.L(T,'C4F10'):8:3,
CON.L(T,'HFC23'):8:3,
