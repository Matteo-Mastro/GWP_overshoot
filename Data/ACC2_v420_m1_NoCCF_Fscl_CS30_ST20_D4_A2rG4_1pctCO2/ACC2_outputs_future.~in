FILE OUT_FUTURE_SCN /out_future_scn.dat/;
OUT_FUTURE_SCN.AP=0;
OUT_FUTURE_SCN.PW=1000;
PUT OUT_FUTURE_SCN;
PUT
'YEAR ',
'ANTCO2 ',
'ANTCH4 ',
'ANTN2O ',
'ANTCO2EQ ',
'ABLCO2 ',
'ABLCH4 ',
'ABLN2O ',
'MACCO2 ',
'MACCH4 ',
'MACN2O ',
'ABCCO2 ',
'ABCCH4 ',
'ABCN2O ',
'CONCO2 ',
'CONCH4 ',
'CONN2O ',
'CONrOH ',
'RADCO2 ',
'RADCH4 ',
'RADN2O ',
'RADO3TRP ',
'RADHALO ',
'RADSF6 ',
'RADSO2 ',
'RADCRBAER ',
'RADAERIND ',
'RADTTL ',
'T2M ',
'pH ',
'DSCCUR ',
'ANTQ1M ',
'ANTQ2M ',
'ANTQ3M '/;
LOOP(T, PUT T.TL:4,
ANT.L(T,'CO2'):10:4,
ANT.L(T,'CH4'):10:2,
ANT.L(T,'N2O'):8:2,
ANTCO2EQ.L(T):8:2,
ABL.L(T,'CO2'):8:2,
ABL.L(T,'CH4'):8:2,
ABL.L(T,'N2O'):8:2,
MAC.L(T,'CO2'):15:2,
MAC.L(T,'CH4'):15:2,
MAC.L(T,'N2O'):15:2,
ABC.L(T,'CO2'):15:2,
ABC.L(T,'CH4'):15:2,
ABC.L(T,'N2O'):15:2,
CON.L(T,'CO2'):9:2,
CON.L(T,'CH4'):9:2,
CON.L(T,'N2O'):9:2,
CON.L(T,'rOH'):9:4,
RAD.L(T,'CO2'):9:4,
RAD.L(T,'CH4'):9:4,
RAD.L(T,'N2O'):9:4,
RAD.L(T,'O3TRP'):9:4,
RADHAL.L(T):9:4,
RAD.L(T,'SF6'):9:4,
RAD.L(T,'SO2'):9:4,
RAD.L(T,'CRBAER'):9:4,
RAD.L(T,'AERIND'):9:4,
RADTTL.L(T):9:4,
T2M.L(T):9:4,
PH.L(T):9:4,
DSCCUR.L(T):13:2,
ANTQ1.M(T):20:6,
ANTQ2.M(T):20:6,
ANTQ3.M(T):20:6/;);


*** File outputs to display results (additional) -----------------------------------------------------------------------

FILE OUT_FUTURE_1 /out_future_1.dat/;
OUT_FUTURE_1.AP=0;
PUT OUT_FUTURE_1;
PUT
'YEAR ',
'ENG ',
'NEG ',
'CONCO2 ',
'OCNCO2UP ',
'LNDCO2UP ',
'ANTCH4 ',
'ANTN2O ',
'CONCH4 ',
'CONN2O ',
'RADTTL ',
'T2MP '/;
LOOP(T, PUT T.TL:4,
ENG.L(T,'CO2'):12:6,
NEG.L(T,'CO2'):12:6,
CON.L(T,'CO2'):12:6,
CO2OCNUPTTL.L(T):12:6,
CO2LNDUPTTL.L(T):12:6,
ANT.L(T,'CH4'):9:3,
ANT.L(T,'N2O'):9:4,
CON.L(T,'CH4'):8:1,
CON.L(T,'N2O'):8:1,
RADTTL.L(T):10:5,
T2M.L(T):10:5/;);

$ontext
FILE OUT_FUTURE_2 /out_future_2.dat/;
OUT_FUTURE_2.AP=0;
PUT OUT_FUTURE_2;
LOOP(TOUT, PUT TOUT.TL:5,
CON.L(TOUT,'CO2'):6:0,
CON.L(TOUT,'CH4'):6:0,
CON.L(TOUT,'N2O'):6:0,
RADTTL.L(TOUT):7:2,
T2MRT1990.L(TOUT):7:2/;);

FILE OUT_FUTURE_3 /out_future_3.dat/;
OUT_FUTURE_3.AP=0;
PUT OUT_FUTURE_3;
LOOP(T, PUT T.TL:5,
COC.L(T,'1'):10:2,
COC.L(T,'2'):10:2,
COC.L(T,'3'):10:2,
COC.L(T,'4'):10:2,
CTR.L(T,'1'):10:2,
CTR.L(T,'2'):10:2,
CTR.L(T,'3'):10:2,
CTR.L(T,'4'):10:2,
SUMCOC.L(T):12:6,
SUMCTR.L(T):12:6/;);

FILE OUT_FUTURE_4 /out_future_4.dat/;
OUT_FUTURE_4.AP=0;
PUT OUT_FUTURE_4;
PUT
'YEAR ',
'CTRREL1 ',
'CTRREL2 ',
'CTRREL3 ',
'CTRREL4 ',
'NPPBAC1 ',
'NPPBAC2 ',
'NPPBAC3 ',
