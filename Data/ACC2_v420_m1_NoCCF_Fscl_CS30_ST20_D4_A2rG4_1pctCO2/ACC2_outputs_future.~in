CON.L(T,'CFC113'):8:3,
CON.L(T,'CFC114'):8:3,
CON.L(T,'CFC115'):8:3,
CON.L(T,'CCl4'):8:3,
CON.L(T,'CH3CCl3'):8:3,
CON.L(T,'Halon1211'):8:3,
CON.L(T,'Halon1301'):8:3,
CON.L(T,'Halon2402'):8:3,
CON.L(T,'HCFC22'):8:3,
CON.L(T,'HCFC141b'):8:3,
CON.L(T,'HCFC142b'):8:3,
